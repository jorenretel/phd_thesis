  keywords = {Automation,Biochemistry, general,Biophysics/Biomedical Physics,Genetic algorithm,Polymer Sciences,Protein NMR,Sequence-specific resonance assignment},
  pages = {127--138},
  file = {Full Text PDF:/zotero/storage/C6NADQRD/Volk et al. - 2008 - Automated sequence-specific protein NMR assignment.pdf:application/pdf;Snapshot:/zotero/storage/XG9IRV49/10.html:text/html}
