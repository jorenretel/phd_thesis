  month = nov,
  year = {2012},
  keywords = {Biochemistry,Biophysics,Structural biology},
  pages = {779--783},
  file = {Full Text PDF:/zotero/storage/ZFKPN2QZ/Park et al. - 2012 - Structure of the chemokine receptor CXCR1 in phosp.pdf:application/pdf;Snapshot:/zotero/storage/J6NGASF5/nature11580.html:text/html}
