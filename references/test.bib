  journal = {Journal of Biomolecular {NMR}},
  author = {Akbey, {\"U}mit and Lange, Sascha and Franks, W. Trent and Linser, Rasmus and Rehbein, Kristina and Diehl, Anne and Rossum, Barth-Jan van and Reif, Bernd and Oschkinat, Hartmut},
  month = aug,
