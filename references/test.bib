  file = {Full Text PDF:/zotero/storage/PE78C646/Kainosho et al. - 2006 - Optimal isotope labelling for NMR protein structur.pdf:application/pdf;Snapshot:/zotero/storage/MZGGF8W4/nature04525.html:text/html}
}

