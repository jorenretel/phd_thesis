  file = {ACS Full Text PDF w/ Links:/zotero/storage/AD282MTT/Linser et al. - 2014 - Solid-State NMR Structure Determination from Diago.pdf:application/pdf;ACS Full Text Snapshot:/zotero/storage/NJDMXCEX/ja504603g.html:text/html}
}

