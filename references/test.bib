  journal = {Nature},
  author = {Loquet, Antoine and Sgourakis, Nikolaos G. and Gupta, Rashmi and Giller, Karin and Riedel, Dietmar and Goosmann, Christian and Griesinger, Christian and Kolbe, Michael and Baker, David and Becker, Stefan and Lange, Adam},
  month = jun,
  year = {2012},
  keywords = {Biophysics,Structural biology},
  pages = {276--279},
  file = {Full Text PDF:/zotero/storage/99G79P8S/Loquet et al. - 2012 - Atomic model of the type III secretion system need.pdf:application/pdf;Snapshot:/zotero/storage/RHN4D7X2/nature11079.html:text/html}
}

@article{Hong2006,
  title = {Electrostatic couplings in {OmpA} ion-channel gating suggest a mechanism for pore opening},
  volume = {2},
  copyright = {{\textcopyright} 2006 Nature Publishing Group},
  issn = {1552-4450},
  url = {http://www.nature.com/nchembio/journal/v2/n11/full/nchembio827.html},
  doi = {10.1038/nchembio827},
  abstract = {The molecular forces that drive structural transitions between the open and closed states of channels and transporters are not well understood. The gate of the OmpA channel is formed by the central Glu52-Arg138 salt bridge, which can open to form alternate ion pairs with Lys82 and Glu128. To gain deeper insight into the channel-opening mechanism, we measured interaction energies between the relevant side chains by double-mutant cycle analysis and correlated these with the channel activities of corresponding point mutants. The closed central salt bridge has a strong interaction energy of -5.6 kcal mol-1, which can be broken by forming the open-state salt bridge Glu52-Lys82 (\ensuremath{{\Delta}{\Delta}}GInter = -3.5 kcal mol-1) and a weak interaction between Arg138 and Glu128 (\ensuremath{{\Delta}{\Delta}}GInter = -0.6 kcal mol-1). A covalent disulfide bond in place of the central salt bridge completely blocks the channel. Growth assays indicate that this gating mechanism could physiologically contribute to the osmoprotection of Escherichia coli cells from environmental stress.},
  language = {en},
  timestamp = {2015-06-10 17:06:09},
  number = {11},
  urldate = {2015-06-10},
  journal = {Nature Chemical Biology},
  author = {Hong, Heedeok and Szabo, Gabor and Tamm, Lukas K.},
  month = nov,
  year = {2006},
  pages = {627--635},
  file = {Snapshot:/zotero/storage/KVCBWZUG/nchembio827.html:text/html}
}

