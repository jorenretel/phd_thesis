  pages = {75--84},
  file = {Full Text PDF:/zotero/storage/NIU8B6N8/Hefke et al. - 2010 - Optimization of amino acid type-specific 13C and 1.pdf:application/pdf;Snapshot:/zotero/storage/CK5D25XQ/10.html:text/html}
