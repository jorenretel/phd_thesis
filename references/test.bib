  keywords = {Broadband homonuclear correlation spectroscopy,COmbined    R    2    n    v     -Driven Spin Diffusion (CORD SD),Fast MAS,R    2    n    v     -Driven Spin Diffusion (RDSD),Solid-state NMR,Symmetry sequences},
