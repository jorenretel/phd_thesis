  journal = {Journal of Biomolecular {NMR}},
  author = {Ward, Meaghan E. and Wang, Shenlin and Krishnamurthy, Sridevi and Hutchins, Howard and Fey, Michael and Brown, Leonid S. and Ladizhansky, Vladimir},
  month = dec,
  year = {2013},
  keywords = {Biochemistry, general,Biophysics and Biological Physics,Condensed data collection,Magic angle spinning,Membrane proteins,Paramagnetic relaxation enhancement,Proton detection,Solid-state NMR,Spectroscopy/Spectrometry},
  pages = {37--47},
  file = {Full Text PDF:/zotero/storage/VVT7QC5A/Ward et al. - 2013 - High-resolution paramagnetically enhanced solid-st.pdf:application/pdf;Snapshot:/zotero/storage/ZSCEI66G/10.html:text/html}
