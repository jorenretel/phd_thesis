  volume = {91},
  issn = {1386-1425},
  url = {http://www.sciencedirect.com/science/article/pii/S1386142512000388},
  doi = {10.1016/j.saa.2012.01.025},
  abstract = {The pH dependent opening and closure of Escherichia coli OmpG is driven by the formation and breaking of hydrogen bridges in \ensuremath{\beta}-strands S11{\textendash}S13. We have investigated the in situ secondary structural changes of OmpG with ATR{\textendash}FTIR difference spectroscopy in order to detect the signals associated with the newly established interactions. Curve-fitting of OmpG in two pH conditions revealed the splitting and shifting of \ensuremath{\beta}-sheet signals upon opening of the channel. Besides secondary structure changes, there are also amino acid side chain signals that play active role in opening/closing of the channel. An interaction among positively charged arginines and negatively charged aspartic and glutamic acid residues is suggested upon closure of the channel while this interaction is abolished when the channel opens at higher pH.},
  timestamp = {2015-06-10 16:36:03},
  urldate = {2015-06-10},
  journal = {Spectrochimica Acta Part A: Molecular and Biomolecular Spectroscopy},
  author = {Korkmaz, Filiz and K{\"o}ster, Stefan and Yildiz, {\"O}zkan and M{\"a}ntele, Werner},
  month = jun,
  year = {2012},
  keywords = {Amide-I band analysis,Curve-fitting,Difference spectrum,FT-IR spectroscopy,Secondary structure determination,\ensuremath{\beta}-barrel membrane protein},
  pages = {395--401},
  file = {ScienceDirect Snapshot:/zotero/storage/AW3EX8MG/S1386142512000388.html:text/html}
}

@article{Asami2012,
  title = {Optimal degree of protonation for 1H detection of aliphatic sites in randomly deuterated proteins as a function of the {MAS} frequency},
  volume = {54},
  issn = {0925-2738, 1573-5001},
  url = {http://link.springer.com/article/10.1007/s10858-012-9659-9},
  doi = {10.1007/s10858-012-9659-9},
  abstract = {The 1H dipolar network, which is the major obstacle for applying proton detection in the solid-state, can be reduced by deuteration, employing the RAP (Reduced Adjoining Protonation) labeling scheme, which yields random protonation at non-exchangeable sites. We present here a systematic study on the optimal degree of random sidechain protonation in RAP samples as a function of the MAS (magic angle spinning) frequency. In particular, we compare 1H sensitivity and linewidth of a microcrystalline protein, the SH3 domain of chicken \ensuremath{\alpha}-spectrin, for samples, prepared with 5{\textendash}25 \% H2O in the E. coli growth medium, in the MAS frequency range of 20{\textendash}60 kHz. At an external field of 19.96 T (850 MHz), we find that using a proton concentration between 15 and 25 \% in the M9 medium yields the best compromise in terms of sensitivity and resolution, with an achievable average 1H linewidth on the order of 40{\textendash}50 Hz. Comparing sensitivities at a MAS frequency of 60 versus 20 kHz, a gain in sensitivity by a factor of 4{\textendash}4.5 is observed in INEPT-based 1H detected 1D 1H,13C correlation experiments. In total, we find that spectra recorded with a 1.3 mm rotor at 60 kHz have almost the same sensitivity as spectra recorded with a fully packed 3.2 mm rotor at 20 kHz, even though {\textasciitilde}20{\texttimes} less material is employed. The improved sensitivity is attributed to 1H line narrowing due to fast MAS and to the increased efficiency of the 1.3 mm coil.},
  language = {en},
  timestamp = {2015-06-10 13:54:37},
  number = {2},
  urldate = {2015-06-10},
  journal = {Journal of Biomolecular {NMR}},
  author = {Asami, Sam and Szekely, Kathrin and Schanda, Paul and Meier, Beat H. and Reif, Bernd},
  month = aug,
  year = {2012},
  keywords = {1.3 mm rotors,2H-labeling,Aliphatic resonances,Biochemistry, general,Biophysics and Biological Physics,Linewidth,Magic angle spinning (MAS) solid-state NMR,Perdeuteration,Polymer Sciences,Reduced Adjoining Protonation (RAP),Sensitivity},
  pages = {155--168},
  file = {Full Text PDF:/zotero/storage/WPKIAXBV/Asami et al. - 2012 - Optimal degree of protonation for 1H detection of .pdf:application/pdf;Snapshot:/zotero/storage/8IZW9A7U/10.html:text/html}
}

@article{Andreas2010,
  title = {Magic Angle Spinning {NMR} Investigation of Influenza A M218-60: Support for an Allosteric Mechanism of Inhibition},
  volume = {132},
  issn = {0002-7863},
  shorttitle = {Magic Angle Spinning {NMR} Investigation of Influenza A M218-60},
  url = {http://dx.doi.org/10.1021/ja101537p},
  doi = {10.1021/ja101537p},
  abstract = {The tetrameric M2 proton channel from influenza A virus conducts protons at low pH and is inhibited by aminoadamantyl drugs such as amantadine and rimantadine (Rmt). We report magic angle spinning NMR spectra of POPC and DPhPC membrane-embedded M218?60, both apo and in the presence of Rmt. Similar line widths in the spectra of apo and bound M2 indicate that Rmt does not have a significant impact on the dynamics or conformational heterogeneity of this construct. Substantial chemical shift changes for many residues in the transmembrane region support an allosteric mechanism of inhibition. An Rmt titration supports a binding stoichiometry of \ensuremath{>}1 Rmt molecule per channel and shows that nonspecific binding or changes in membrane composition are unlikely sources of the chemical shift changes. In addition, doubling of spectral lines in all of the observed samples provides evidence that the channel assembles with twofold symmetry.},
  timestamp = {2015-06-10 15:11:22},
  number = {32},
  urldate = {2015-06-10},
  journal = {Journal of the American Chemical Society},
  author = {Andreas, Loren B. and Eddy, Matthew T. and Pielak, Rafal M. and Chou, James and Griffin, Robert G.},
  month = aug,
  year = {2010},
  pages = {10958--10960},
  file = {ACS Full Text PDF w/ Links:/zotero/storage/3T434G3S/Andreas et al. - 2010 - Magic Angle Spinning NMR Investigation of Influenz.pdf:application/pdf;ACS Full Text Snapshot:/zotero/storage/DQQ9B9U4/ja101537p.html:text/html}
}

