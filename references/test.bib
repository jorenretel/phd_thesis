  month = jul,
  year = {2009},
  keywords = {Automated structure determination,Biochemistry, general,Biophysics/Biomedical Physics,CYANA,FLYA,Polymer Sciences,SAIL,Structure validation},
  pages = {261--272},
  file = {Full Text PDF:/zotero/storage/RDUA33JA/Ikeya et al. - 2009 - Automated NMR structure determination of stereo-ar.pdf:application/pdf;Snapshot:/zotero/storage/T4S2J4DB/10.html:text/html}
}

