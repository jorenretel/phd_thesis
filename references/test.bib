  month = jul,
  year = {2012},
  keywords = {copper,microcrystal,nuclear relaxation rates,Paramagnetism},
  pages = {11095--11100},
  file = {Full Text PDF:/zotero/storage/NTEVUFNT/Knight et al. - 2012 - Structure and backbone dynamics of a microcrystall.pdf:application/pdf;Snapshot:/zotero/storage/83T7EHH4/11095.html:text/html}
}

