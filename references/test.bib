  keywords = {membrane insertion,membrane protein folding,membrane protein stability,outer membrane protein,VDAC},
  pages = {66--78},
  file = {ScienceDirect Snapshot:/zotero/storage/MFCHSW6Q/S0022283607001301.html:text/html}
}

