  month = mar,
  year = {2005},
  keywords = {Dielectric,Heating,Probe,Protein,Salt,Scroll,Solenoid,solid-state NMR spectroscopy},
  pages = {40--48},
  file = {ScienceDirect Snapshot:/zotero/storage/KUBI78HB/S1090780704003830.html:text/html}
}

