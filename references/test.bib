  month = jul,
  year = {2009},
  keywords = {Biochemistry, general,Biophysics/Biomedical Physics,Isotopic labelling,Membrane proteins,Polymer Sciences,protein structure determination,Resonance assignment,Solid-state NMR},
  pages = {245--260},
