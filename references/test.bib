  volume = {213},
  issn = {0022-2836},
  url = {http://www.sciencedirect.com/science/article/pii/S0022283605801937},
  doi = {10.1016/S0022-2836(05)80193-7},
  abstract = {An equation for calculating the distances between the atoms involved in forming an idealized hydrogen bond in a parallel or antiparallel \ensuremath{\beta}-barrel has been derived by adjusting the corresponding data given by Pauling and Corey for a \ensuremath{\beta}-sheet. Based on these distances, a geometrical optimization method was developed, by which one can generate various idealized \ensuremath{\beta}-barrels: parallel or antiparallel, tilted or non-tilted, right-tilted or left-tilted. For each type of idealized \ensuremath{\beta}-barrel thus obtained, the corresponding conformation and characteristic geometric parameters as well as their relationship are analyzed and discussed. Since the strand in a tilted \ensuremath{\beta}-barrel traces a curve rather than a straight line on a cylinderlike surface, a regular chain in which the dihedral angles of each residue are the same cannot form a tilted \ensuremath{\beta}-barrel but only a non-tilted \ensuremath{\beta}-barrel. As observed, the strands of a right-tilted \ensuremath{\beta}-barrel possess a very strong right-handed twist. The radii of the idealized tilted parallel and antiparallel \ensuremath{\beta}-barrels are greater than those of the corresponding non-tilted ones by \ensuremath{\sim} 1 {\AA} and \ensuremath{\sim} 1\ensuremath{\cdot}5 {\AA}, respectively. Consequently, there is relatively more room for a tilted \ensuremath{\beta}-barrel to accommodate the internal side-chains, suggesting that a conformational change from a non-tilted \ensuremath{\beta}-barrel to a tilted one would ease the repulsion among the crowded internal side-chains so as to make the structure more stable. The values of root-mean-square fits indicate that the idealized right-tilted \ensuremath{\beta}-barrels coincide quite well with the observed \ensuremath{\beta}-barrels in both parallel and antiparallel cases.},
  timestamp = {2015-06-10 13:33:12},
  number = {2},
  urldate = {2015-06-10},
  journal = {Journal of Molecular Biology},
  author = {Chou, Kuo-Chen and Carlacci, Louis and Maggiora, Gerald G.},
  month = may,
  year = {1990},
  pages = {315--326},
  file = {ScienceDirect Full Text PDF:/zotero/storage/4HPJACVF/Chou et al. - 1990 - Conformational and geometrical properties of ideal.pdf:application/pdf;ScienceDirect Snapshot:/zotero/storage/2IK85WA4/S0022283605801937.html:text/html}
}

@article{Lomize2006a,
  title = {Positioning of proteins in membranes: A computational approach},
  volume = {15},
  copyright = {Copyright {\textcopyright} 2006 The Protein Society},
  issn = {1469-896X},
  shorttitle = {Positioning of proteins in membranes},
  url = {http://onlinelibrary.wiley.com/doi/10.1110/ps.062126106/abstract},
  doi = {10.1110/ps.062126106},
  abstract = {A new computational approach has been developed to determine the spatial arrangement of proteins in membranes by minimizing their transfer energies from water to the lipid bilayer. The membrane hydrocarbon core was approximated as a planar slab of adjustable thickness with decadiene-like interior and interfacial polarity profiles derived from published EPR studies. Applicability and accuracy of the method was verified for a set of 24 transmembrane proteins whose orientations in membranes have been studied by spin-labeling, chemical modification, fluorescence, ATR FTIR, NMR, cryo-microscopy, and neutron diffraction. Subsequently, the optimal rotational and translational positions were calculated for 109 transmembrane, five integral monotopic and 27 peripheral protein complexes with known 3D structures. This method can reliably distinguish transmembrane and integral monotopic proteins from water-soluble proteins based on their transfer energies and membrane penetration depths. The accuracies of calculated hydrophobic thicknesses and tilt angles were \ensuremath{\sim}1 {\AA} and 2{\textdegree}, respectively, judging from their deviations in different crystal forms of the same proteins. The hydrophobic thicknesses of transmembrane proteins ranged from 21.1 to 43.8 {\AA} depending on the type of biological membrane, while their tilt angles with respect to the bilayer normal varied from zero in symmetric complexes to 26{\textdegree} in asymmetric structures. Calculated hydrophobic boundaries of proteins are located \ensuremath{\sim}5 {\AA} lower than lipid phosphates and correspond to the zero membrane depth parameter of spin-labeled residues. Coordinates of all studied proteins with their membrane boundaries can be found in the Orientations of Proteins in Membranes (OPM) database:http://opm.phar.umich.edu/.},
  language = {en},
  timestamp = {2015-06-10 13:34:15},
  number = {6},
  urldate = {2015-06-10},
  journal = {Protein Science},
  author = {Lomize, Andrei L. and Pogozheva, Irina D. and Lomize, Mikhail A. and Mosberg, Henry I.},
  month = jun,
  year = {2006},
  keywords = {channel,database,membrane protein,modeling,receptor,rhodopsin,tilt,transporter},
  pages = {1318--1333},
  file = {Full Text PDF:/zotero/storage/ATRW7BTQ/Lomize et al. - 2006 - Positioning of proteins in membranes A computatio.pdf:application/pdf;Snapshot:/zotero/storage/ZHBEMJZ5/abstract\;jsessionid=2239C659125299649D23D5EA7704BD76.html:text/html}
}

