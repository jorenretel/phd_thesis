  volume = {1565},
  issn = {0005-2736},
  url = {http://www.sciencedirect.com/science/article/pii/S0005273602005771},
  doi = {10.1016/S0005-2736(02)00577-1},
  abstract = {Integral membrane proteins come in two types, \ensuremath{\alpha}-helical and \ensuremath{\beta}-barrel proteins. In both types, all hydrogen bonding donors and acceptors of the polypeptide backbone are completely compensated and buried while nonpolar side chains point to the membrane. The \ensuremath{\alpha}-helical type is more abundant and occurs in cytoplasmic (or inner) membranes, whereas the \ensuremath{\beta}-barrels are known from outer membranes of bacteria. The \ensuremath{\beta}-barrel construction is described by the number of strands and the shear number, which is a measure for the inclination angle of the \ensuremath{\beta}-strands against the barrel axis. The common right-handed \ensuremath{\beta}-twist requires shear numbers slightly larger than the number of strands. Membrane protein \ensuremath{\beta}-barrels contain between 8 and 22 \ensuremath{\beta}-strands and have a simple topology that is probably enforced by the folding process. The smallest barrels form inverse micelles and work as enzymes or they bind to other macromolecules. The medium-range barrels form more or less specific pores for nutrient uptake, whereas the largest barrels occur in active Fe2+ transporters. The \ensuremath{\beta}-barrels are suitable objects for channel engineering, because the structures are simple and because many of these proteins can be produced into inclusion bodies and recovered therefrom in the exact native conformation.},
  timestamp = {2015-06-10 17:07:11},
  number = {2},
  urldate = {2015-06-10},
  journal = {Biochimica et Biophysica Acta ({BBA}) - Biomembranes},
  author = {Schulz, Georg E.},
  month = oct,
  year = {2002},
  keywords = {Chainfold topology,Channel engineering,shear number,\ensuremath{\beta}-Barrel,\ensuremath{\beta}-Helix,\ensuremath{\beta}-Twist},
  pages = {308--317},
  file = {ScienceDirect Full Text PDF:/zotero/storage/BT7UWJSQ/Schulz - 2002 - The structure of bacterial outer membrane proteins.pdf:application/pdf;ScienceDirect Snapshot:/zotero/storage/785GT58Z/S0005273602005771.html:text/html}
}

@article{Demers2011,
