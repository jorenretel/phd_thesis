  keywords = {Biochemistry, general,Biophysics and Biological Physics,Dynamic nuclear polarization (DNP),Magic-angle spinning NMR (MAS-NMR),Ribosome structure,Solid-state NMR,Spectroscopy/Spectrometry,TOTAPOL},
  pages = {85--93},
  file = {Full Text PDF:/zotero/storage/4NN37GQX/Gelis et al. - 2013 - Solid-state NMR enhanced by dynamic nuclear polari.pdf:application/pdf;Snapshot:/zotero/storage/6C8NZDSB/10.html:text/html}
