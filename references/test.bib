  number = {1},
  urldate = {2015-06-10},
  journal = {Journal of Molecular Biology},
  author = {Shanmugavadivu, Baladhandapani and Apell, Hans-J{\"u}rgen and Meins, Thomas and Zeth, Kornelius and Kleinschmidt, J{\"o}rg H.},
  month = apr,
