  journal = {Journal of Biomolecular {NMR}},
  author = {Hefke, Frederik and Bagaria, Anurag and Reckel, Sina and Ullrich, Sandra Johanna and D{\"o}tsch, Volker and Glaubitz, Clemens and G{\"u}ntert, Peter},
