  keywords = {data model, nuclear magnetic resonance, CcpNmr applications,format conversion,high throughput,Software},
  pages = {687--696},
  file = {Full Text PDF:/zotero/storage/A88Q9NCQ/Vranken et al. - 2005 - The CCPN data model for NMR spectroscopy Developm.pdf:application/pdf;Snapshot:/zotero/storage/FQZJGPB2/abstract.html:text/html}
