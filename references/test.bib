  pages = {289--302},
  file = {Full Text PDF:/zotero/storage/9HGIVAI3/Cornilescu et al. - 1999 - Protein backbone angle restraints from searching a.pdf:application/pdf;Snapshot:/zotero/storage/CVZKP4F3/10.html:text/html}
