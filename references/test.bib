@article{Pauli2000,
  title = {Sample Optimization and Identification of Signal Patterns of Amino Acid Side Chains in 2D {RFDR} Spectra of the \ensuremath{\alpha}-Spectrin {SH}3 Domain},
  volume = {143},
  issn = {1090-7807},
  url = {http://www.sciencedirect.com/science/article/pii/S1090780700920299},
  doi = {10.1006/jmre.2000.2029},
  abstract = {Future structural investigations of proteins by solid-state CPMAS NMR will rely on uniformly labeled protein samples showing spectra with an excellent resolution. NMR samples of the solid \ensuremath{\alpha}-spectrin SH3 domain were generated in four different ways, and their 13C CPMAS spectra were compared. The spectrum of a {[}u-13C, 15N{]}-labeled sample generated by precipitation shows very narrow 13C signals and resolved scalar carbon{\textendash}carbon couplings. Linewidths of 16{\textendash}19 Hz were found for the three alanine C\ensuremath{\beta} signals of a selectively labeled {[}70\% 3-13C{]}alanine-enriched SH3 sample. The signal pattern of the isoleucine, of all prolines, valines, alanines, and serines, and of three of the four threonines were identified in 2D 13C{\textendash}13C RFDR spectra of the {[}u-13C,15N{]}-labeled SH3 sample. A comparison of the 13C chemical shifts of the found signal patterns with the 13C assignment obtained in solution shows an intriguing match.},
