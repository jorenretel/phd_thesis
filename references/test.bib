  keywords = {dipole CSA relaxation interference,Magic angle spinning,MAS solid-state NMR,perdeuterated proteins,TROSY},
  pages = {143--156},
  file = {Full Text PDF:/zotero/storage/SPJ3ZS8U/Chevelkov and Reif - 2008 - TROSY effects in MAS solid-state NMR.pdf:application/pdf;Snapshot:/zotero/storage/QSWDSHRH/abstract.html:text/html}
}

