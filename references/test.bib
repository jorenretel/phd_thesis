  month = apr,
  year = {2006},
  pages = {959--962},
  file = {Full Text PDF:/zotero/storage/5K9FG7SM/Lange et al. - 2006 - Toxin-induced conformational changes in a potassiu.pdf:application/pdf;Snapshot:/zotero/storage/3CVQMPNC/nature04649.html:text/html}
}

